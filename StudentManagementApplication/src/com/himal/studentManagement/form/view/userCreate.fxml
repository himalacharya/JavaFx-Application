<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.himal.studentManagement.form.ctrl.UserCreateController">
   <children>
      <GridPane alignment="center" hgap="10" vgap="10" styleClass="VBox" stylesheets="@main.css">
         <columnConstraints>
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
         <children>
            <AnchorPane prefHeight="500.0" prefWidth="500.0">
               <children>
                  <Label fx:id="nameLabel" layoutX="66.0" layoutY="81.0" text="Name" />
                  <Label fx:id="idLabel" layoutX="66.0" layoutY="123.0" text="ID" />
                  <Label fx:id="marksLabel" layoutX="39.0" layoutY="164.0" prefHeight="17.0" prefWidth="77.0" text="Password" />
                  <Label fx:id="contactLabel" layoutX="66.0" layoutY="209.0" text="Contact" />
                  <TextField fx:id="nameTextField" layoutX="116.0" layoutY="77.0" />
                  <TextField fx:id="idTextField" layoutX="116.0" layoutY="128.0" />
                  <TextField fx:id="contactTextField" layoutX="116.0" layoutY="205.0" />
                  <Button fx:id="save" layoutX="146.0" layoutY="238.0" mnemonicParsing="false" onAction="#saveAction" prefHeight="25.0" prefWidth="77.0" text="Save" />
                  <Button fx:id="reset" layoutX="265.0" layoutY="238.0" mnemonicParsing="false" onAction="#resetAction" prefHeight="25.0" prefWidth="77.0" text="Reset" />
                  <PasswordField fx:id="passwordTextField" layoutX="116.0" layoutY="169.0" />
               </children>
            </AnchorPane>
         </children>
      </GridPane>
   </children>
</VBox>
